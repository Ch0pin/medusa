pathClassLoader.$init.overloads[0].implementation= function( dexPath,  parent){
    colorLog('PathClassLoader called:', {c: Color.Green});
    console.log("dexPath=" + dexPath );
    console.log("parent=" + parent);         
    return this.$init(dexPath, parent);

}




